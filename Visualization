#visualization 

import pandas as pd
import numpy as np

df = pd.DataFrame(np.random.randn(10,4), index=pd.date_range("1/1/2000",periods=10), columns=list('ABCD'))

print(df.plot())


#area plot
#import pandas as pd
#import numpy as np

df = pd.DataFrame(np.random.rand(10,4), columns =['a', 'b', 'c','d'])
print(df.plot.box())
df.plot.area()
